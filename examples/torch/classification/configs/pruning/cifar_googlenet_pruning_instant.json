{
    "model": "googlenet_cifar",
    "batch_size" : 128,
    "pretrained": true,
    "epochs": 120,
    "input_info": {
        "sample_size": [
            1,
            3,
            32,
            32
        ]
    },
    "optimizer": {
        "type": "SGD",
        "base_lr": 1e-2,
        "weight_decay": 5e-4,
        "schedule_type": "multistep",
        "steps": [
            60,
            90
        ],
        "optimizer_params":
        {
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "compression": [
       {
            "algorithm": "filter_pruning",
            "pruning_init": 0.7,
            "params": {
                "pruning_target": 0.7
            }
       }
    ]
}
