{
    "model": "ResNet18_cifar",
    "pretrained": true,
    "batch_size" : 256,
    "epochs": 200,
    "input_info": {
      "sample_size": [1, 3, 32, 32]
    },
    "optimizer": {
        "type": "SGD",
        "base_lr": 0.1,
        "weight_decay": 5e-4,
        "schedule_type": "multistep",
        "steps": [
            60,
            120,
            160
        ],
        "optimizer_params":
        {
            "momentum": 0.9,
            "nesterov": true
        }
    },
   "compression": [
       {
           "algorithm": "filter_pruning",
           "pruning_init": 0.5,
            "params": {
                "schedule": "exponential",
                "pruning_target": 0.5,
                "pruning_steps": 15,
                "weight_importance": "L2"
            }
        }
   ]
}
