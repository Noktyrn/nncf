{
    "model": "ResNet50_cifar",
    "pretrained": true,
    "batch_size" : 256,
    "epochs": 120,
    "input_info": {
      "sample_size": [1, 3, 32, 32]
    },
    "optimizer": {
        "type": "SGD",
        "base_lr": 1e-2,
        "weight_decay": 5e-4,
        "schedule_type": "multistep",
        "steps": [
            60,
            90
        ],
        "optimizer_params":
        {
            "momentum": 0.9,
            "nesterov": true
        }
    },
   "compression": [
       {
            "algorithm": "filter_pruning",
            "pruning_init": 0.5
        }
   ]
}