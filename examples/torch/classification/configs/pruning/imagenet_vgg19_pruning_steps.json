{
    "model": "vgg19_tiny",
    "pretrained": true,
    "batch_size" : 128,
    "epochs": 90,
    "num_classes": 200,
    "input_info": {
      "sample_size": [1, 3, 64, 64]
    },
    "optimizer": {
        "type": "SGD",
        "base_lr": 1e-2,
        "weight_decay": 5e-4,
        "schedule_type": "multistep",
        "steps": [
            30,
            60,
            75
        ],
        "optimizer_params":
        {
            "momentum": 0.9,
            "nesterov": true
        }
    },
    "compression": [
        {
            "algorithm": "filter_pruning",
            "pruning_init": 0.1,
             "params": {
                 "schedule": "exponential",
                 "pruning_target": 0.7,
                 "pruning_steps": 15,
                 "weight_importance": "L2"
             }
         }
    ]
}